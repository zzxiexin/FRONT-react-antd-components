{
  "name": "modern-ui-component-library",
  "version": "1.0.0",
  "description": "A modern React UI component library with CSS-in-JS architecture",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --module commonjs --outDir dist --declaration true",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "dev": "vite",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "component-library",
    "css-in-js",
    "theme",
    "modern-ui"
  ],
  "author": "Your Name",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "classnames": "^2.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}