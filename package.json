{
  "name": "dynamic-theme-component-library",
  "version": "1.0.0",
  "description": "A React component library with dynamic theme switching support",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "webpack --config webpack.config.js --mode production",
    "build:esm": "webpack --config webpack.esm.config.js --mode production",
    "dev": "webpack serve --config webpack.dev.config.js",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "component-library",
    "theme",
    "css-variables",
    "dynamic-theme"
  ],
  "author": "Your Name",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "core-js": "3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.18.0",
    "@babel/preset-typescript": "^7.18.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "babel-loader": "^9.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.0",
    "eslint": "^8.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.0.0",
    "less": "^4.1.0",
    "less-loader": "^11.1.0",
    "style-loader": "^3.3.0",
    "typescript": "^4.9.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.0"
  }
}
